	width: $main_width;
	margin: 30px auto 0;

	.header{
		@extend .clearfix;
		background-color: darken($page_bg, 5px);
		font-size: 20px;
		text-transform: uppercase;
		font-weight: 400;
		font-style: italic;

		h2{
			color: darken($page_bg, 15px);
			float: left;
			font-size: 20px;
			padding: 20px 30px;
			font-weight: 400;
			letter-spacing: 8px;
		}

		.add-comment-btn{
			background-color: $text_accent;
			color: $text_main;
			padding: 20px 30px;
			float: right;
			cursor: pointer;

			&:hover{
				background-color: $page_bg;
			};
		}
	}
}

.comment-form-holder{
	background-color: darken($page_bg, 15px);
	padding: 30px;
	display: none;

	form{
		width: 400px;
		margin: 0 auto;
		padding: 30px;
	}
}

.comments{
	width: 600px;
	margin: 0 auto;
	padding: 30px 0;
}
.comment{
	width: 430px;
	background-color: darken($page_bg, 5%);
	margin-bottom: 20px;

	.comment-header{
		background-color: darken($page_bg, 7%);
		padding: 10px;
		font-weight: 400;
		font-style: italic;

		.title a{
			font-size: 18px;
			color: $text_main;
			float: left;
		}

		.website a{
			color: $text_accent;
			float: right;
			text-transform: uppercase;
			font-size: 14px;

			&:hover{
				color: $text_main;
			};
		}
	}

	.submitted{
		padding: 5px 10px;
		background-color: darken($page_bg, 2%);
		font-size: 10px;
		text-transform: uppercase;
	}

	.body{

	}
	
	.field-name-comment-body{
		padding: 20px 20px 20px 60px;
		font-size: 14px;
	}

	.quo-mark{
		position: absolute !important;
		font-size: 80px;
		padding: 0 20px;
		color: darken($page_bg, 15%);
	}
}




/* SERVICE LINKS
-------------------------------------------------------------- */
.service-links{

	li{
		float: left;
		margin-left: 10px;
	}
}

/* FOOTER
-------------------------------------------------------------- */
$footer_height: 		50px;

* {
  margin: 0;
}
html, body {
  height: 100%;
}
#wrapper {
  min-height: 100%;
  /* equal to footer height */
  margin-bottom: -$footer_height; 
}
#wrapper:after {
  content: "";
  display: block;
}
#footer, #wrapper:after {
  /* .push must be the same height as footer */
  height: $footer_height; 
}
#footer { 
	@include font($font_header);
	position: relative;
	margin-top: -$footer_height;
	height: $footer_height;
	clear:both;
	background-color: $text_lt;
	z-index: -999;

	text-align: center;
	line-height: $footer_height;
	font-size: 12px;
	text-transform: uppercase;
	font-weight: 400;
	letter-spacing: 2px;
}

/* SOCIAL
-------------------------------------------------------------- */
.social-icons{

	.icon{
		
		background: #fff url(../images/bb_social.png) no-repeat;
		height: 60px;
		width: 60px;
		display: inline-block;
		border-radius: 30px;
		margin: 10px 0 0 10px;

		transition: background-color $trans ease;
		&:hover{background-color: $text_accent;}
	}

	.twitter{background-position: -60px 0;}
	.instagram{background-position: -120px 0;}
	.youtube{background-position: -90px 0;}
	.pinterest{background-position: -120px 0;}
}



/* FORMS
-------------------------------------------------------------- */
form{
	background-color: $page_bg;
}

.form-item{margin-top: 0;}

input[type="text"],
input[type="password"],
input[type="email"],
textarea{
	width: 98%;
	padding: 5px 1%;
	border: none;
	background-color: lighten($page_bg, 10%);
	font-size: 18px;
	font-family: $font_main;
	font-weight: 400;
	color: lighten($text_main, 50%);
	box-shadow: 1px 1px 1px #ccc inset;

	&:focus{
		background-color: lighten($page_bg, 5%);
		color: $text_main;
	};
}

label{
	font-size: 12px;
	text-transform: uppercase;
}

input[type="submit"]{
	background-color: $text_accent;
	color: $text_main;
	padding: 15px 20px 10px;
	cursor: pointer;
	text-transform: uppercase;
	font-size: 14px;
	letter-spacing: 2px;
	font-style: italic;
	border: none;
	margin-left: 5px;

	&:hover{
		background-color: lighten($page_bg, 10%);
	};
}

.form-actions{
	text-align: right;
	margin-bottom: 0;
}




/* EXTRAS
-------------------------------------------------------------- */
body.cke_show_borders{width: 740px !important; min-width: 740px; margin: 0 auto;}

.image-post_full {
	position: relative;
	z-index: 1;
}
.hover-pinterest {
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 999;
	background-image: url('../images/bb_pinit.png');
	background-repeat: no-repeat;
	background-position: bottom right;
	display: none;
	width: 100%;
	height: 100%;
	opacity: .8;

	&:hover{
		opacity: 1;
	};
	
}
.pin-it-link {
	height: 100%;
	width: 100%;
	display: block;
}


/* MOBILE
-------------------------------------------------------------- */
@media only screen and (max-width: 1344.4px) {
	#main{
		width: 90%;
	}
}


$gutter_mobile: 		10px;


@media only screen and (max-width: 768px) {
	body{}

	#main{
		width: 90%;
		margin: 0 auto;
	}
	#header{
		text-align: center;
		margin: 0 auto;
		width: 90%;
	}

	#logo{
		padding: $gutter_mobile 0;
		width: 100%;
		margin: 0 auto;
	}


	#nav{
		display: none;

		li{
			list-style: none;
			float: none;
			margin: 0;
			padding: 0;
			text-align: center;
		}
	}

	.menu-toggle{display: block; margin-bottom: 10px;}
	
	.gallery-full .images .field-name-field-gallery-images .field-item {
		position: relative !important;
		display: block !important;
		top: auto;
		left: auto; 
		line-height: 0;
		margin-bottom: 10px;

	}

	.gallery-full .images .field-name-field-gallery-images .field-item img{
		width: 100%;
		height: auto;
	}

	#controls{display: none; visibility: hidden;}

	.view-id-videos{
		/*@extend .clearfix;*/
		padding: 10px;
	}
	
	#overview-content{
		
		.frame-big{
			padding: $gutter_mobile * 4;
		}

		p{
			margin-bottom: $gutter_mobile;
		}

		.caption,
		.actions{
			float: none;
			text-align: center;
		}


	}


}

@media only screen and (max-width: 500px) {
	#overview-content{
		
		.frame-big{
			padding: $gutter_mobile * 2;
		}

		p{
			width: 80%;
		}
		
		.quote-small{
			width: 100%;
		}


	}
}









